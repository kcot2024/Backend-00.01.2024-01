
Proceso Ejercicio11
	
	definir numero1, numero2, numero3, mayornumero como real 
	
	Escribir "Escriba el primer numero "
    Leer numero1
    
    Escribir "Escriba el segundo número "
    Leer numero2
    
    Escribir "Escriba el tercer número "
    Leer numero3
    
	si numero1 >= numero2 y numero1 >= numero3 Entonces
		mayornumero = numero1
	sino 
		si numero2 >= numero1 y numero2 >= numero3 Entonces
			mayornumero = numero2
		SiNo
			mayornumero = numero3
		FinSi
	FinSi
    
    Escribir "El numero mayor es :", mayornumero
    
FinProceso