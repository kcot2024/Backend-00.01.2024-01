Proceso Conversiones
	Definir centimetros, pulgadas, libras, kilogramos Como Real
	cm_pulgadas = 0.39
    libras_kg = 0.45
	
    Escribir "Ingrese longitud (cm): "
    Leer centimetros
	
    Escribir "Ingrese peso (lb): "
    Leer libras
	pulgadas = centimetros * cm_pulgadas
    kilogramos = libras * libras_kg
	Escribir "Longitud: ", pulgadas,"pulgadas"
    Escribir "Peso: ", kilogramos, "kg"
FinProceso
